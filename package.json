{
  "name": "gatsby-starter-p5",
  "description": "A starter for Gatsby.js with p5.js.",
  "version": "0.1.0",
  "scripts": {
    "clean": "gatsby clean",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "deploy": "npm run clean && gatsby build --prefix-paths && gh-pages -d public"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.6",
    "gatsby": "^2.18.12",
    "gatsby-plugin-react-helmet": "^3.1.18",
    "gatsby-plugin-styled-components": "^3.1.16",
    "p5": "^0.10.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-icons": "^3.8.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "gh-pages": "^2.1.1",
    "husky": "^3.1.0",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
